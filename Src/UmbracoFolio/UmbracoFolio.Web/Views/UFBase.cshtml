@inherits Umbraco.Web.Mvc.UmbracoViewPage
@{
    Layout = null;

    var settings = UmbracoFolio.Web.Helpers.SettingsHelper.GetSiteSettings(Model);
    var cachePeriod = 60 * 60; // 1 hour
}
<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">

    <title>UmbracoFolio</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <meta property="og:title" content="">
    <meta property="og:type" content="">
    <meta property="og:url" content="">
    <meta property="og:image" content="">

    <link rel="manifest" href="site.webmanifest">
    <link rel="apple-touch-icon" href="icon.png">
    <!-- Place favicon.ico in the root directory -->

    <link href="~/assets/scss/umbracofolio.css" rel="stylesheet" />
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Lato:ital,wght@0,100;0,300;0,400;0,700;0,900;1,100;1,300;1,400;1,700;1,900&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Pacifico&display=swap" rel="stylesheet">
</head>

<body class="umbracofolio">
    @Html.CachedPartial("~/Views/Partials/UF/_Header.cshtml", settings, cachePeriod, cacheByPage: true)

    @RenderBody()

    <script src="~/assets/js/vendor/uikit.js"></script>
    <script src="~/assets/js/vendor/uikit-icons.js"></script>
</body>

</html>